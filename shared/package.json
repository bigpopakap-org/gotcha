{
  "name": "@gotcha/shared",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "watch": {
    "build": {
      "patterns": [
        "src",
        "test"
      ],
      "extensions": "json,js,jsx,ts,d.ts,tsx"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "dev": "npm-watch build",
    "test": "echo \"No tests implemented yet\"",
    "lint": "yarn eslint --ext json,js,jsx,ts,d.ts,tsx ./",
    "lintfix": "yarn lint --fix",
    "build": "tsc"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "2.3.3",
    "@typescript-eslint/parser": "2.3.3",
    "eslint": "6.5.1",
    "eslint-config-bigpopakap": "0.2.0",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-json": "1.4.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-typescript": "0.14.0",
    "npm-watch": "0.6.0",
    "prettier": "1.18.2",
    "typescript": "3.6.4"
  }
}
